package day1210;

public class Ex4DataType {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int year=2024;
		int month =12;
		int day= 11;
		int year2 =2024;
		int month2 =5;
		int day2= 3;
		
		System.out.println(year+"-"+month+"-"+day);
		System.out.printf("%d-%d-%d\n",year2,month2,day2);
		//printf를 사용해 println의 2023-05-03처럼 표기(달 일,앞에 0이 붙음)
		
		System.out.printf("%d-%02d-%02d",year2,month2,day2);
		
		
		int money = 2356000;
		System.out.printf("이번달 급여는 =%10d원 \n",money);
		System.out.printf("이번달 급여는 =%-10d원\n",money);
		
	}

}
