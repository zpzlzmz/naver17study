package day1212;

import java.util.Scanner;

public class Ex14If {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int score;
		String grade;
		System.out.println("점수를 입력해 주세요");
		score=sc.nextInt(); 
		if(score>=90)
			grade = "A";
		else if(score>=80)
			grade = "B";
		else if(score>=70)
			grade = "C";
		else if(score>=60)
			grade = "D";
		else
			grade = "F";
		
		System.out.printf("당신의 학점은 %s 입니다",grade);
	}

}
