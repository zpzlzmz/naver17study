package day1213;

import java.util.Scanner;

public class Ex4Switch {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		 Scanner sc = new Scanner(System.in);
		 int num;
	
		 System.out.println("숫자 입력");
		 num=sc.nextInt();
		 switch(num%2) {
		 
		 case 0:
			 System.out.println("짝수입니다.");
			 break;
		 case 1:
			 System.out.println("짝수입니다.");
			 break;
			 
		 }
	
	}
	

}
