package day1210;
import java.util.Scanner;


public class Main {

		public static void main(String[] args) {
			// TODO Auto-generated method stub
			Scanner sc=new Scanner(System.in);
			long sum = 0;
			for(int i =0; i<3; i++) {
				long a = sc.nextLong();
				sum += a;
			}
			System.out.println(sum);
	}
}
		

