package day1210;

public class Ex7DataType {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String name="candy";
		int age=32;
		float height = 163.8f;
		double weight = 56.7;
		char blood='B';
		/*
		 * 이름:canady님
		 * 나이:32세
		 * 키:163.8
		 * 몸무게:56.7
		 * 혈액형:B
		 * 출력하시오
		 */
		
		System.out.printf("이름:%s님\n나이:%d세\n키:%.1f\n몸무게:%.1f\n혈액형:%c\n\n",name,age,height,weight,blood);
		
		System.out.println("이름:"+name+"님\n나이:"+age+"세\n키:"+height+"cm\n몸무게:"+weight+"kg\n혈액형:"+blood+"형\n");
	
		
		
	
	
	}
	
	

}
